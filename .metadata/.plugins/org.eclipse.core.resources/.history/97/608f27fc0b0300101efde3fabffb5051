package w3school_Example;

public class Day250311_Overloading {

	static int plusMethod(int x, int y) {
		return x + y;
	}
	
	
	static double plusMethod(double x, double y, double z) {
		return x + y + z;
	}
	
	public static void main(String[] args) {

		int plus = plusMethod(5, 5);
		double db = plusMethod(5.5, 6.6, 5.0);
		
		System.out.println("int : " + plus); 
		System.out.println("double : " + db);
		
	}

}
