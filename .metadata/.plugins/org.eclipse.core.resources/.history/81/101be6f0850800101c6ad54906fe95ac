package com.mystudy.innerclass;

public class StaticClassTest {
	int a = 100;
	private int b = 10;
	static int c = 200;
	int sum;

	void sum() {
		sum = a + b;
	}

	// 내부클래스 : 인스턴스 클래스
	static class Inner {
		static int in = 100;
		int e = 2000;
		
		void printData() { //non - static 메소드
			System.out.println("외부 int a : " + a);
		}
	}

	public static void main(String[] args) {

	}

}
