
public class Ex05_max_value {

	public static void main(String[] args) {

		// (실습) 숫자 3개 중 가장 큰 수를 구하시오
		// a : 30 , b : 60 , c : 20
		
		//< 결과 >
		//가장 큰 수 : 60
		
		
		int a = 30;
		int b = 60;
		int c = 20;
		
		int max = 0;
		
		
		if (a > b) {
			if (a > c) {
				max = a;
			}
			else if (c > a) {
				max = c;
			}
		}
	}

}
