import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class TodoFilter {

	// 제목으로 todo 검색
	// 완료 / 미완료 구분 출력
	// 우선순위 todo 항목 필터링

	public void search(List<Todo> list) {
		Scanner filter = new Scanner(System.in);

		System.out.print("찾을 제목을 입력해주세요 : ");
		String todoName = filter.nextLine();

		boolean found = false;

		for (int i = 0; i < list.size(); i++) {
			Todo todo = list.get(i);
			if (todo.getTitle().contains(todoName)) {
				
			}
		}

	}
}
